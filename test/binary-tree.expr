letrec (
  leaf = lambda () {
    lambda (x) {
      if (.== x 0) then 0
      else exit
    }
  }
  node = lambda (val left right) {
    lambda (x) {
      if (.== x 0) then 1
      else if (.== x 1) then val
      else if (.== x 2) then left
      else right
    }
  }
  dfs = lambda (tree) {
    if (.== (tree 0) 0) then (.void)
    else [
      (dfs (tree 2))
      (.put (tree 1) "\n")
      (dfs (tree 3))
    ]
  }
) {
  (dfs
    (node 4
      (node 2
        (node 1 (leaf) (leaf))
        (node 3 (leaf) (leaf))
      )
      (node 5 (leaf) (leaf))
    )
  )
}
